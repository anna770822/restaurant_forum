<%= form_for [:admin, @restaurant] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :tel %>
  <%= f.text_field :tel %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :opening_hours %>
  <%= f.text_field :opening_hours %>

  <%= f.label :description %> 
  <%= f.text_area :description %>

  <%= f.submit %>
<% end %>
